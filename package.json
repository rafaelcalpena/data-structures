    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^3.0.8",
    "karma-webpack": "^4.0.0-beta.0",
    "ng-packagr": "^5.4.3",
    "null-loader": "^0.1.1",
    "semantic-release": "^15.5.0",
    "ts-loader": "^4.3.0",
    "tslint": "^5.9.1",
    "typedoc": "^0.12.0",
    "typescript": "^3.5.3",
    "webpack": "4.10.1",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2",
    "lodash": "^4.17.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
